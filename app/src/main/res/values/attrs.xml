<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TostTimer">
        <attr name="reverse" format="boolean" />
        <attr name="duration_millis" format="integer" />
    </declare-styleable>

    <declare-styleable name="ProblemToolBar">
        <attr name="title" format="string" />
    </declare-styleable>

    <declare-styleable name="AudioStateButton">
        <attr name="state" format="enum">
            <enum name="RECORDING" value="0" />
            <enum name="STOP" value="1" />
            <enum name="PLAYING" value="2" />
            <enum name="PAUSE" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="WeeklyGoalDisplayHome">
        <attr name="count" format="integer" />
        <attr name="part" format="integer" />
    </declare-styleable>
</resources>